{
  "name": "ansi-regex",
  "version": "6.2.2",
  "description": "Regular expression for matching ANSI escape codes",
  "type": "module",
  "sideEffects": false,
  "author": "Mike Nicholaides <mike@nicholaides.com>",
  "license": "MIT",
  "dependencies": {
    "ansi-escapes": "7.1.1"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./test-cases": {
      "types": "./dist/cjs/test-cases.d.ts",
      "require": "./dist/cjs/test-cases.js",
      "import": "./dist/esm/test-cases.js"
    },
    "./fixtures/ansi-codes": {
      "types": "./dist/cjs/fixtures/ansi-codes.d.ts",
      "require": "./dist/cjs/fixtures/ansi-codes.js",
      "import": "./dist/esm/fixtures/ansi-codes.js"
    }
  },
  "devDependencies": {
    "@danielx/civet": "^0.10.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.3",
    "esbuild": "^0.25.11",
    "mocha": "^11.2.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "./build/compile",
    "clean": "rm -rf ./dist",
    "test": "mocha",
    "view-supported": "node fixtures/view-codes.js"
  },
  "mocha": {
    "extension": ["civet"],
    "import": ["@danielx/civet/register"],
    "recursive": true,
    "spec": ["**/*.test.civet"]
  }
}
